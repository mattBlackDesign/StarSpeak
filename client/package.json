{
  "name": "react-webpack-rails-tutorial",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.9.0",
    "npm": "4.1.1"
  },
  "scripts": {
    "watch-test": "karma start",
    "lint": "eslint .",
    "autofix": "eslint . --fix",
    "test": "yarn run lint && yarn run test-offline",
    "test-offline": "karma start karma.conf.js --log-level debug --single-run",
    "test-integration": "TEST_MODE=integration karma start --single-run",
    "build:test": "webpack --config webpack.config.js",
    "build:production": "yarn && NODE_ENV=production webpack --config webpack.config.js",
    "build:development": "webpack -w --config webpack.config.js"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
